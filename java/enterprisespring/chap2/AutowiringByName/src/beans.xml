<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="employeeServiceBean" 
		class="cn.org.aris.java.enterprisespring.chap2.autowire.autowirebyname.EmployeeService" 
		autowire="byName">
	</bean>

	<bean id="employee" 
		class="cn.org.aris.java.enterprisespring.chap2.autowire.autowirebyname.Employee">
		<property name="employeeId" value="1065"></property>
	</bean>
</beans>
